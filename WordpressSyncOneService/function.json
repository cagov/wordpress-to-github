{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "WordpressSync",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0/2 * * * *"
    }
  ]
}
