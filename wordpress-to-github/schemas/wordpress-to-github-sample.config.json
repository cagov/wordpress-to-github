{
  "$schema": "./wordpress-to-github.config.schema.json",
  "meta": {
    "title": "wordpress-to-github endpoints config file",
    "description": "wordpress-to-github endpoints config file"
  },
  "data": {
    "wordpress_source_url": "https://as-go-covid19-d-001.azurewebsites.net",
    "github_target": {
      "template":{
        "disabled": false,
        "PostPath": "wordpress_output/posts",
        "PagePath": "wordpress_output/pages",
        "MediaPath": "wordpress_output/media",
        "ExcludeProperties": [
          "content",
          "_links"
        ]
      },
      "targets":[
        {
          "outputBranch": "main",
          "tags_exclude": [
            "staging",
            "development"
          ]
        },
        {
          "outputBranch": "staging",
          "tags_exclude": [
            "development"
          ]
        }
      ]
    }
  }
}